{
  "name": "frontend-website",
  "version": "0.1.0",
  "private": true,
  "description": "yidayun website",
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next build && next export",
    "lint": "next lint",
    "lint:code": "eslint . --ext .js,.jsx,.mjs,.cjs,.ts,.tsx,.cts,.mts --fix",
    "prettier": "prettier --write \"**/*.(js|jsx|cjs|mjs|ts|tsx|cts|mts|json)\""
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@headlessui/react": "^1.7.2",
    "@splinetool/runtime": "^0.9.106",
    "animejs": "^3.2.1",
    "autoprefixer": "^10.4.11",
    "classnames": "^2.3.2",
    "daisyui": "^2.28.0",
    "next": "12.3.0",
    "next-seo": "^5.5.0",
    "postcss": "^8.4.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-reveal": "^1.2.2",
    "sass": "^1.54.9",
    "swiper": "^8.4.2",
    "tailwindcss": "^3.1.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "eslint": "8.22.0",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "typescript": "4.8.3"
  }
}
